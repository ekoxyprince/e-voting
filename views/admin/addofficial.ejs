<%-include("../partials/adminheader")%>
<div class="main-content">
    <div class="col-xs-12 col-sm-12 col-md-9 col-lg-9">
        <div class="card">
            <div class="boxs mail_listing">
                <div class="inbox-center table-responsive">
                    <table class="table table-hover">
                        <thead>
                            <tr>
                                <th colspan="1">
                                    <div class="inbox-header">
                                     Add New Official
                                    </div>
                                </th>
                            </tr>
                        </thead>
                    </table>
                </div>
                <div class="row">
                    <div class="col-lg-12">
                        <form class="composeForm" action="/admin/add_officials" method="post">
                            <div class="form-group">
                                <div class="form-line">
                                    <input type="text" name="name" id="email_address" class="form-control" placeholder="Official Name" value="" required>
                                </div>
                            </div>
                           
                            <div class="form-group">
                                <div class="form-line">
                                    <input type="text" name="password" id="subject" class="form-control" value="" placeholder="Enter Password" required>
                                </div>
                            </div>
                          
                            <div class="col-lg-12 mt-3">
                                <div class="m-l-25 m-b-20">
                                    <button type="submit" class="btn btn-info btn-border-radius waves-effect">Send</button>
                                    <button type="reset" class="btn btn-danger btn-border-radius waves-effect">Discard</button>
                                </div>
                            </div>
                        </form>
                    </div>

                </div>
            </div>
        </div>
    </div>

<%-include("../partials/adminfooter")%>